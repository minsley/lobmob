# Track images with Git LFS
030-knowledge/assets/**/*.png filter=lfs diff=lfs merge=lfs -text
030-knowledge/assets/**/*.jpg filter=lfs diff=lfs merge=lfs -text
030-knowledge/assets/**/*.jpeg filter=lfs diff=lfs merge=lfs -text
030-knowledge/assets/**/*.gif filter=lfs diff=lfs merge=lfs -text
030-knowledge/assets/**/*.webp filter=lfs diff=lfs merge=lfs -text
030-knowledge/assets/**/*.svg filter=lfs diff=lfs merge=lfs -text
030-knowledge/assets/**/*.pdf filter=lfs diff=lfs merge=lfs -text
